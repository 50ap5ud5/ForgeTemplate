{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",

  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": ["${mod_authors}"],
  "contact": {
    "homepage": "${homepage}",
    "sources": "${sources}"
  },

  "license": "${license}",
  "icon": "${icon_file}",

  "environment": "*",
  "entrypoints": {
    "main": [
      "${main_mod_class}"
    ]
  },
  "accessWidener":"${mod_id}.accesswidener",
  "mixins": [
    "${mod_id}.mixins.json"
  ],

  "depends": {
    "fabricloader": "${fabric_loader_version_range}",
    "fabric": "${fabric_version_range}",
    "minecraft": "${minecraft_version_range}",
    "java": "${java_version_range}"
  },
  "suggests": {
    "cloth-config2": "*",
    "mod-menu":"*"
  }
}
